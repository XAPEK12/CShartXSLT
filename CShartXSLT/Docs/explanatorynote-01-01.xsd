            <xs:enumeration value="10.2">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">10.2) подвесные канатные дороги</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="11а">
