            <xs:documentation xml:lang="ru">Кадастровый номер участка</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d+:\d+:\d+:\d+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tFileName">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка длиной до 255 символов - имя файла</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:maxLength value="255"/>
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tEmail">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка - адрес электронной почты</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:pattern value="[a-zA-Zа-яА-Я0-9_.\-]{1,}[@]{1}[a-zA-Zа-яА-Я0-9_.\-]{1,}[.]{1}[a-zA-Zа-яА-Я]{2,}"/>
