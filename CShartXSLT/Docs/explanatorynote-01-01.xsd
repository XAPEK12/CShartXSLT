        <xs:restriction base="xs:normalizedString">
            <xs:maxLength value="200"/>
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="string500">
        <xs:annotation>
